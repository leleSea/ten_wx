 .test-style{
  width: 50px;
  height: 50px;
  background: #336CD3;
  opacity: 0.3;
}










page{
  background: #EEF1F7;
}
/* view, text, cover-view, button{
  font-family: 'PingFang' !important;
} */
view{
  box-sizing: border-box;
  font-family: TrebuchetMS,Rotobo,"Microsoft YaHei",sans-serif;
  /* font-family:'PingFangSC-Light' !important; */
}
swiper{
  box-sizing: border-box;
}
swiper-item{
  box-sizing: border-box;
}
cover-view{
  box-sizing: border-box;
  font-family: TrebuchetMS,Rotobo,"Microsoft YaHei",sans-serif;
  white-space: normal;
}
button{
   box-sizing: border-box;
   font-family: TrebuchetMS,Rotobo,"Microsoft YaHei",sans-serif;
}
text{
  font-family: TrebuchetMS,Rotobo,"Microsoft YaHei",sans-serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
  margin:0px auto;
} 
.board-touch{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 100;
  top: 0px;
  left: 0px;
}
.board canvas {
  /* width:710rpx;
  height:710rpx; */
  width: 100%;
  height: 100%;
}
.board{
  box-shadow: 0px 6rpx 6rpx #C6C8CD;
  background:url(http://comment.weiqiai.com/img/wood1.jpg);
  position:relative;
  left:0px;
  right:0px;
  width:100%;
  z-index: 0;
  transform-origin:center top;
}
.board-canvas{
  position:absolute;
  top:0px;
  left:0px;
  z-index:1;
}
.board-stone{
  position:absolute;
  top:0px;
  left:0px;
  z-index:2;
}
.board-an{
  position:absolute;
  top:0px;
  left:0px;
  z-index:2;
}
.board-label{
  position:absolute;
  top:0px;
  left:0px;
  z-index:2;
}
board-time {
top:0px;
left:0px;
z-index:2;
}

.rank-page{
  overflow: hidden;
  width: 100%;
}
.limit-text{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.relative{
  position: relative;
}
.rotate180{
  transform: rotate(180deg)
}
.new-function::before{
  content:'new';
  position: absolute;
  top: 0px;
  right: 0px;
  background: #f83838;
  color: #fff;
  font-size: 14rpx;
  width:40rpx;
  height:25rpx;
  display:flex;
  align-items:center;
  justify-content:center;
}
.hidden{
  display: none !important;
}
.htlp-notice{
  position:fixed;
  z-index:1000;
  background:rgba(0,0,0,0.6);
  width:100%;
  height:100%;
  padding:20rpx;
  color:#fff;
  /* display: flex; */
  align-items: center;
}
.htlp-notice .title{
  text-align:center;
  /* position:absolute; */
  top:20rpx;
  width:100%;
  font-size: 40rpx;
}
.htlp-notice .content{
  font-size: 32rpx;
  margin-top: 30rpx;
}
.htlp-notice .content .item{
  margin-top: 10rpx;
  text-indent:2em;
  width: 100%;
}
.pull-left{
  float: left;
}
button .tool-num{
  font-size: 9px;
}
.over-hidden{
  overflow: hidden;
}
.rotate180{
  transform: rotate(180deg);
}
.defineBtn.button-hover{
  background: #336CD3 !important;
  color: #fff !important;
}
/* button[disabled]:not(type){
  opacity: 0.5;
  background:#EEF1F7;
  color:#778097;
} */
.clear-margin{
  margin: 0px !important;
}

.topbar{
  border-top: 1px solid #EEF1F7;
  width: 100%;
  height: 1px;
  position: fixed;
  top: 0px;
  left: 0px;
}
.text-win{
  color: #0CB34E;
}
.text-lose{
  color: #F46161;
}
.border-modal{
  background: url(http://comment.weiqiai.com/img/setboardmodal.png) no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
}
.page-content{
  width: 100%;
}
.winrate-chart{
  width: 100%;
  height: 160rpx;
  display: flex;
  align-items: center;
}
.winrate-chart .winrate-chart-a{
  width: 100%;
  height: 100%;
}
button.common-btn{
  font-size: 24rpx;
  width: 128rpx;
  height: 64rpx;
  padding: 0px;
  background: #EEF1F7;
  color: #212A42;
  border: none;
  outline: none;
  float: left;
  line-height: normal;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
}
button.common-btn.button-hover{
  background: #336CD3;
  color: #EEF1F7;
}
button.common-btn[disabled]:not(type){
  background: #EEF1F7;
  color: #A0A7BB;
  opacity: 1;
}
button.common-btn.select{
  background: #336CD3;
  color: #EEF1F7;
  font-weight: 600;
}